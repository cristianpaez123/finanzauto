<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/userListFragment">

    <fragment
        android:id="@+id/userListFragment"
        android:name="com.example.myapplication.iu.view.UserListFragment"
        tools:layout="@layout/fragment_list_user">
        <action
            android:id="@+id/action_listUserFragment_to_RegisterUserFragment"
            app:destination="@id/registerUserFragment" />
        <action
            android:id="@+id/action_listUserFragment_to_UserDetailFragment"
            app:destination="@id/UserDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/registerUserFragment"
        android:name="com.example.myapplication.iu.view.RegisterUserFragment"
        tools:layout="@layout/fragment_register_user">
    </fragment>
    <fragment
        android:id="@+id/UserDetailFragment"
        android:name="com.example.myapplication.iu.view.UserDetailFragment"
        tools:layout="@layout/fragment_user_detail">
    </fragment>
</navigation>